SELECT DISTINCT  *
WHERE
  { SELECT DISTINCT  *
    WHERE
      { { SELECT  ?result
          WHERE
            { { SELECT  *
                    WHERE
                      { ?result  a                     <https://w3id.org/won/model#Need> ;
                                 <https://w3id.org/won/model#isInState>  <https://w3id.org/won/model#Active> ;
                                 <https://w3id.org/won/model#seeks>  ?seeks .
                        ?seeks <https://w3id.org/won/model#travelAction>/<http://schema.org/fromLocation> ?fromLocation .
                        ?fromLocation
                                  <http://schema.org/geo>  ?fromLocation_geo
                      }
                    ORDER BY ?fromLocation_geoDistance
                  }
                  FILTER NOT EXISTS { ?result  <https://w3id.org/won/model#hasFlag>  <https://w3id.org/won/model#NoHintForCounterpart> }
                
            }
          OFFSET  0
          LIMIT   20
        }
      }
  }
